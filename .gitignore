erl_crash.dump

# Also ignore archive artifacts (built via "mix archive.build").
*.ez

# Ignore package tarball (built via "mix hex.build").
no_way_jose-*.tar

/native/nowayjose/target
/native/nowayjose_testutils/target

/priv/native
/target
